{
  "name": "mockachino",
  "version": "0.2.3",
  "description": "Generate mock data for your apps. Take a sip and relax",
  "main": "src/Mockachino.js",
  "author": {
    "name": "Simao Nziaka",
    "email": "sdnziaka@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/akaizn-junior/mockachino"
  },
  "keywords": [
    "mock",
    "fake",
    "data",
    "seed",
    "mockachino",
    "faker",
    "mocker"
  ],
  "scripts": {
    "test:u": "mocha 'src/unit.spec.js' --require @babel/register",
    "test": "nyc npm run test:unit",
    "lint": "eslint .",
    "compile": "rm -R dist; export NODE_ENV='production'; webpack --progress",
    "watch": "rm -R dev; webpack --watch",
    "build": "npm run lint && npm run test:cov && npm run make:js",
    "serveBase": "webpack-dev-server",
    "docs": "jsdoc src/Mockachino.js -c jsdoc.json --verbose && cp logo.gif docs",
    "serve": "webpack-dev-server -w",
    "pack": "./test_package.sh",
    "size": "npx size-limit"
  },
  "size-limit": [
    {
      "path": "src/Mockachino.js",
      "limit": "7 KB"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/register": "^7.4.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "dotenv": "^7.0.0",
    "eslint": "^5.15.3",
    "jsdoc": "^3.5.5",
    "minami": "^1.2.3",
    "mocha": "^6.0.2",
    "sinon": "^7.2.7",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  }
}
